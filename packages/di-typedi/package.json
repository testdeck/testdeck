{
  "name": "@testdeck/di-typedi",
  "version": "0.1.2",
  "description": "TypeDI IOC integration for testdeck",
  "author": "Panayot Cankov",
  "contributors": [
    {
      "name": "silkentrance",
      "url": "https://github.com/silkentrance"
    }
  ],
  "homepage": "https://testdeck.org/",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/testdeck/testdeck.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "prepare": "tsc",
    "pretest": "tsc",
    "test": "nyc mocha dist/test.js && tslint --project .",
    "tslint": "tslint --project .",
    "tslint-fix": "tslint --project . --fix"
  },
  "devDependencies": {
    "@testdeck/core": "^0.1.2",
    "@testdeck/mocha": "^0.1.2",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.8.0"
  },
  "files": [
    "dist/index.js",
    "dist/index.js.map",
    "README.md",
    "LICENSE"
  ],
  "nyc": {
    "check-coverage": true,
    "lines": 95,
    "statements": 85,
    "functions": 95,
    "branches": 95,
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "dist",
      "coverage",
      "test/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true
  },
  "publishConfig": {
    "access": "public"
  }
}
